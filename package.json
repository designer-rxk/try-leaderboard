{
  "name": "mono-v3-starter",
  "version": "0.0.1",
  "bin": {
    "mono": "bin/cli.js"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo run dev --filter=@mono/web",
    "dev:studio": "turbo run dev --filter=@mono/studio",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.14",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=20",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8"
  }
}
